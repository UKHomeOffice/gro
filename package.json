{
  "name": "GRO",
  "version": "1.0.1",
  "main": "server.js",
  "description": "The General Register Office DSP form",
  "license": "GPL-2.0",
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOffice/gro/"
  },
  "readme": "./README.md",
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development hof-build watch --env",
    "test": "npm run test:lint",
    "test:acceptance": "TAGS=\"${TAGS:=@feature}\" npm run test:cucumber",
    "test:acceptance_browser": "ACCEPTANCE_WITH_BROWSER=true TAGS=\"${TAGS:=@feature}\" yarn run test:cucumber",
    "test:cucumber": "cucumber-js -f @cucumber/pretty-formatter \"test/_features/**/*.feature\" --require test/_features/test.setup.js --require \"test/_features/step_definitions/**/*.js\" --tags $TAGS",
    "test:integration": "NODE_ENV=development _mocha \"test/_integration/**/*.spec.js\" -r dotenv/config --exit",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test",
    "build": "hof-build",
    "postinstall": "yarn run build"
  },
  "author": "HomeOffice",
  "dependencies": {
    "browserify": "^17.0.0",
    "deprecate": "^1.0.0",
    "hof": "^20.0.4",
    "homeoffice-countries": "^0.1.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "typeahead-aria": "^1.0.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "chai": "^4.3.6",
    "debug": "^4.3.2",
    "eslint": "^7.30.0",
    "eslint-config-hof": "^1.0.4",
    "express": "^4.14.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "playwright": "1.23.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.0",
    "sinon-chai": "^3.7.0",
    "snyk": "^1.675.0",
    "supertest": "^6.2.2",
    "supertest-session": "^4.1.0"
  },
  "resolutions": {
    "underscore": "^1.12.1"
  },
  "mocha": {
    "reporter": "spec",
    "require": "test/setup.js",
    "recursive": "true",
    "timeout": "6000"
  }
}
